{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "plugin:react/jsx-runtime"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/no-cycle": [
            0,
            {
                "ignoreExternal": true
            }
        ],
        "no-underscore-dangle": [
            0,
            {
                "allowAfterThis": true
            }
        ],
        "linebreak-style": [
            0,
            "windows"
        ],
        "indent": [
            0,
            "tab"
        ],
        "react/jsx-indent": [
            0,
            "tab"
        ]
    }
}